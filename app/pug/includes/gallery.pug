-
    const slides = [
        'img/Rectangle 5.png',
        'img/Rectangle 9.png',
        'img/Rectangle 5.png',
        'img/singapore-noodles.jpg',
        'img/singapore-noodles.jpg',
        'img/Rectangle 42.png'
    ];

section.gallery#section(data-section-index=3)
    include tabs
    .swiper-container.gallery-container.gallery-container--active(data-item-type='Play-area-for-children')
        .swiper-wrapper.gallery-wrapper
            each slide in slides
                .swiper-slide.gallery-slide
                    a(href=slide data-fancybox="Play-area-for-children")
                        img(src=slide)
        .swiper-button-prev.gallery-button-prev
            <svg width="10" height="18" viewBox="0 0 10 18" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M9 17L0.999999 9L9 1" stroke="#333333"/>
            </svg>
        .swiper-button-next.gallery-button-next
            <svg width="10" height="18" viewBox="0 0 10 18" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M1 1L9 9L1 17" stroke="#333333"/>
            </svg>
